{
    "env": {
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi":["warn","never"],
        "quotes":["warn","single"],
        "space-infix-ops": ["warn", { "int32Hint": false }],
        "no-unused-vars":"off",
        "space-in-parens":"warn",
        "space-unary-ops":"warn",
        "arrow-spacing":"warn",
        "spaced-comment":"warn",
        "no-multi-spaces":"warn",
        "no-undef":"off",
        "no-useless-catch":"off"
    }
}
